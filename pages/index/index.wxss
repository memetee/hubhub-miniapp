#wrap {
    height: 100vh;
    display: flex;
    flex-direction: column;
}

#wrap .body {
    flex: 1;
    height: 100%;
    overflow: auto;
}

#wrap .footer {
    height: 78rpx;
    display: flex;
    justify-content: space-between;
    border-top: 0.8px solid #cecece;
    padding: 6rpx 80rpx;
}

#wrap .footer .footer-item {
    position: relative;
    width: 100rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    font-size: 20rpx;
    color: #ccc;
}

#wrap .footer .footer-item image {
    display: inline-block;
    width: 50rpx;
}

#wrap .footer .footer-item .footer-increment {
    position: absolute;
    top: -50rpx;
    display: inline-block;
    width: 85rpx;
    height: 85rpx;
    border: 1px;
    padding: 12rpx;
    background-color: white;
    border-top: 1px solid #ccc;
    border-radius: 50%;
}

#wrap .footer .footer-item i {
    width: 40rpx;
    height: 40rpx;
    margin-bottom: 5rpx;
}
#wrap .footer .footer-home-active,
#wrap .footer .footer-mine-active {
    color: #d84532;
}

#wrap .footer .footer-home i {
    background: url('data:image/svg+xml;base64,') no-repeat center/100% 100%;
}

#wrap .footer .footer-home-active i {
    background: url('data:image/svg+xml;base64,') no-repeat center/100% 100%;
}

#wrap .footer .footer-mine i {
    background: url('data:image/svg+xml;base64,') no-repeat center/100% 100%;
}

#wrap .footer .footer-mine-active i {
    background: url('data:image/svg+xml;base64,') no-repeat center/100% 100%;
}